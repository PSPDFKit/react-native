{
  "name": "native-catalog",
  "version": "2.5.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./*.js __tests__/*.js ./helpers ./examples ./configuration",
    "refresh": "yarn upgrade react-native-pspdfkit",
